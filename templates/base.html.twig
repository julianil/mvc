<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <style>@import url('https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,400;0,500;0,700;1,300;1,400&display=swap');</style>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
         <nav>
            <ul>
                <li><a href="{{ path('home') }}">Home</a></li>
                <li><a href="{{ path('about') }}">About</a></li>
                <li><a href="{{ path('report') }}">Report</a></li>
                <li><a href="{{ path('lucky') }}">Lucky</a></li>
                <li><a href="{{ path('card_init_get') }}">Card</a></li>
                <li><a href="{{ path('game_init_get') }}">Game</a></li>
                <li><a href="{{ path('app_library') }}">Bibliotek</a></li>
            </ul>
        </nav>
        <header class="site-header" style="background-image: url({{ asset('img/header.jpg') }})"></header>
        <main>{% block body %}{% endblock %}</main>

        <footer>{% block footer %}
        <a href="https://github.com/julianil/mvc">Mitt Git-repo</a><br><br>
        Â© Julia Nilsson{% endblock %}</footer>
    </body>
</html>

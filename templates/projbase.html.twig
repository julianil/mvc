<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <style>@import url('https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,400;0,500;0,700;1,300;1,400&display=swap');</style>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
        <script src="https://cdn.anychart.com/releases/v8/js/anychart-base.min.js"></script>
        <script src="https://cdn.anychart.com/releases/v8/js/anychart-ui.min.js"></script>
        <script src="https://cdn.anychart.com/releases/v8/js/anychart-exports.min.js"></script>
        <link href="https://cdn.anychart.com/releases/v8/css/anychart-ui.min.css" type="text/css" rel="stylesheet">
        <link href="https://cdn.anychart.com/releases/v8/fonts/css/anychart-font.min.css" type="text/css" rel="stylesheet">
        <link rel="preconnect" href="https://fonts.googleapis.com"> 
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin> 
        <link href="https://fonts.googleapis.com/css2?family=Playfair+Display:ital,wght@0,400;0,500;0,600;0,700;0,800;1,400;1,500;1,600;1,700;1,800&display=swap" rel="stylesheet">
        <style type="text/css">

            html,
            body,
            #container {
            width: 100%;
            height: 100%;
            margin: 0;
            padding: 0;
            }
        
        </style>
    </head>
    <body>
         <nav class="nav-proj">
            <ul>
                <li><a href="{{ path('home') }}">Home</a></li>
                <li><a href="{{ path('projekt') }}">Projekt</a></li>
                <li><a href="{{ path('aboutprojekt') }}">About</a></li>
                <li><a href="{{ path('projekt_api') }}">API</a></li>
            </ul>
        </nav>
        <header class="site-header" style="background-image: url({{ asset('img/projheader.jpg') }})"></header>
        <main class="proj-body">{% block body %}{% endblock %}</main>

        <footer class="footer-proj">{% block footer %}
        <a href="https://github.com/julianil/mvc">Mitt Git-repo</a><br><br>
        Â© Julia Nilsson{% endblock %}</footer>
    </body>
</html>
